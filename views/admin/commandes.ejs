<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../css/admin/loader.css">
    <link rel="stylesheet" href="../css/admin/ini.css">
    <link rel="stylesheet" href="../css/admin/inbox.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>commandes</title>
</head>

<body>
    <%- include("../components/admin/loader.ejs") %>

        <%- include("../components/admin/nav.ejs",{acceuil:"",Categories:"",produits:"",settings:"",marques:"",commandes:"active"})
            %>
            <main>
                <%- include("../components/admin/header.ejs",{acceuil:"active",Categories:"",produits:"",settings:"",marques:"",commandes:""})
                    %>
                    <% if ( typeof msg !='undefined' ) { %>
                        <div class="alert <%= type %>">
                            <div class="msg">
                                <%= msg %>
                            </div>
                            <div class="close-btn">
                                <i class="fa fa-times"></i>
                            </div>
                        </div>
                        <% } %>
                    
                            <div class="inbox-sec">
                                <div class="messages">
                                    <div class="message">
                                        <div class="top-sec">
                                            <div class="icon"><img src="https://ui-avatars.com/api/?background=random&name=LouisAdkins&format=svg" alt=""></div>
                                            <div class="name">Louis Adkins</div>
                                        </div>
                                        <div class="min-msg">How to write better advertising copy heaidng</div>
                                        <div class="bt-sc">
                                            <div class="c-bt">
                                                <input id="checkbox" type="checkbox" name="checkbox" />
                                                
                                                <button><i class="fa fa-star"></i></button>
                                            </div>
                                            <div class="dateTime">8:30 AM</div>
                                        </div>
                                    </div>
                                    <div class="message">
                                        <a href="">
                                            <div class="top-sec">
                                                <div class="icon"><img src="https://ui-avatars.com/api/?background=random&name=ClarenceHogan&format=svg" alt="">
                                                </div>
                                                <div class="name">Clarence Hogan</div>
                                            </div>
                                            <div class="min-msg">Old Fashioned recipe for preventing allergies and chemical</div>
                                            </a>
                                            <div class="bt-sc">
                                                <div class="c-bt">
                                                   
                                                    <button><i class="fa fa-star"></i></button>
                                                </div>
                                                <div class="dateTime">Dec 1,2020</div>
                                            </div>
                                       
                                    </div>
                                    <div class="message">
                                        <div class="top-sec">
                                            <div class="icon"><img src="https://ui-avatars.com/api/?background=random&name=OlgaBoone&format=svg" alt="">
                                            </div>
                                            <div class="name">Olga Boone</div>
                                        </div>
                                        <div class="min-msg">How to write better advertising </div>
                                        <div class="bt-sc">
                                            <div class="c-bt">
                                                <input id="checkbox" type="checkbox" name="checkbox" />
                                                
                                                <button><i class="fa fa-star"></i></button>
                                            </div>
                                            <div class="dateTime">Nov 5,2020</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="display-message">
                                    <%- include("../components/admin/svg.ejs") %>

                                </div>
                            </div>
            </main>
            <script src="../javascript/closebtn.js"></script>
            <script src="../javascript/loader.js"></script>

</body>

</html>