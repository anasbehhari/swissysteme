                        <section id="Add-cat">
                            <div class="title">Ajouter une catégorie</div>
                            <form action method="POST">
                                <div>
                                    <label for="cat-name">Nom de catégorie</label>
                                    <input required type="text" placeholder=" Entrer nom catégorie ici " id="catName"
                                        name="catName" value="<%= typeof value != 'undefined' ? value : '' %>">
                                </div>
                                <button type="submit">Ajouter</button>
                            </form>
                        </section>
                        <section id="Add-cat">
                            <div class="title">Supprimer une catégorie</div>
                            <form action method="POST">
                                <div>
                                    <label for="cat-id">Id de catégorie</label>
                                    <input required type="text" placeholder=" Entrer  Id catégorie ici " id="catid"
                                        name="catid" value="<%= typeof value2 != 'undefined' ? value2 : '' %>">
                                </div>
                                <div class="cpt">
                                    <label for="check" class="pt">Supprimer tous les produits appartiennent à cette
                                        catégorie</label>
                                    <input type="checkbox" id="check" name="check">
                                </div>

                                <button type="submit">Supprimer</button>
                            </form>
